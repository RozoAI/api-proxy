{
  "name": "api-proxy",
  "version": "1.0.0",
  "description": "API proxy server that forwards requests and logs them",
  "main": "dist/server.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:env": "ts-node-dev --respawn --transpile-only --env-file env.dev src/server.ts",
    "dev:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0",
    "typescript": "^5.8.3",
    "mysql2": "^3.14.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.13",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  },
  "keywords": [
    "api",
    "proxy",
    "forward",
    "logging",
    "typescript"
  ],
  "author": "",
  "license": "MIT"
}
