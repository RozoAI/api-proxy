{
  "name": "api-proxy",
  "version": "1.0.0",
  "type": "module",
  "description": "Payment API proxy on Supabase Edge Functions with multi-provider support and automatic withdrawal integration",
  "main": "supabase/functions/payment-api/index.ts",
  "scripts": {
    "type-check": "cd supabase && npx tsc --noEmit --project tsconfig.json",
    "lint": "eslint \"supabase/functions/**/*.ts\"",
    "lint:fix": "eslint \"supabase/functions/**/*.ts\" --fix",
    "format": "prettier --write \"supabase/functions/**/*.ts\"",
    "format:check": "prettier --check \"supabase/functions/**/*.ts\"",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:migrate": "supabase db push",
    "supabase:functions:deploy": "supabase functions deploy",
    "supabase:functions:serve": "supabase functions serve --env-file .env.local",
    "deploy": "chmod +x deploy-supabase.sh && ./deploy-supabase.sh"
  },
  "dependencies": {
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5"
  },
  "keywords": [
    "supabase",
    "edge-functions",
    "payment-api",
    "multi-provider",
    "withdrawal-integration",
    "daimo",
    "aqua",
    "cryptocurrency",
    "typescript"
  ],
  "author": "",
  "license": "MIT"
}
