{
  "name": "api-proxy",
  "version": "1.0.0",
  "description": "API proxy server that forwards requests and logs them",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:env": "ts-node-dev --respawn --transpile-only --env-file env.dev src/server.ts",
    "dev:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "alchemy-sdk": "^3.6.3",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "viem": "^2.37.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "api",
    "proxy",
    "forward",
    "logging",
    "typescript"
  ],
  "author": "",
  "license": "MIT"
}
